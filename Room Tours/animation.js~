$(document).ready(function(){
  $("#1").mouseenter(function(){
   $("#a").slideDown();
   
	$("#1").animate({
     
      width:'+=800px'
    });
  });
});
$(document).ready(function(){
  $("#1").mouseleave(function(){
  $("#a").slideUp();
  
    $("#1").animate({
     
      width:'-=800px'
    });
  });
});
